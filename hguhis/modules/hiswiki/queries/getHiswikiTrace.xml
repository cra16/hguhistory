<query id="getHiswikiTrace" action="select">
<!--//지희: HiswikiTraced에 있던 데이터를 가져온다-->
    <tables>
        <table name="hiswiki_trace" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="trace_srl" var="trace_srl" filter="number" notnull="notnull" />
    </conditions>
</query>