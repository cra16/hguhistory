<?xml version="1.0" encoding="UTF-8"?>
<query id="getHiswikiTitle" action="select">
	<tables>
		<table name="hiswiki_doc" />
	</tables>
	<columns>
		<column name="topic" />
	</columns>
    <conditions>
		<condition operation="equal" column="topic" var="topic" />
    </conditions>
</query>