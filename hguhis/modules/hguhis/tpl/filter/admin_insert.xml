<filter name="admin_insert" module="hguhis" act="procHguhisModuleInsert" confirm_msg_code="confirm_submit">
	<form>
		<node target="mid" required="true" maxlength="40" filter="alpha_number" />
		<node target="browser_title" required="true" maxlength="250" />
	</form>
	<response callback_func="completeAdminInsertHguhis">
        <tag name="error" />
        <tag name="message" />
        <tag name="module" />
        <tag name="act" />
        <tag name="page" />
        <tag name="module_srl" />
    </response>
</filter>